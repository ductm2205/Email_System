<div class="pagination">
    <% if (currentPage> 1) { %>
        <a href="<%= baseUrl %>?page=<%= currentPage - 1 %>">&laquo; Previous</a>
        <% } %>

            <% for(let i=1; i <=totalPages; i++) { %>
                <a href="<%= baseUrl %>?page=<%= i %>" class="<%= currentPage === i ? 'active' : '' %>">
                    <%= i %>
                </a>
                <% } %>

                    <% if (currentPage < totalPages) { %>
                        <a href="<%= baseUrl %>?page=<%= currentPage + 1 %>">Next &raquo;</a>
                        <% } %>
</div>